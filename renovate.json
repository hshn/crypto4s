{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchPackageNames": [
        "sbt",
        "sbt/sbt"
      ],
      "groupName": "sbt"
    },
    {
      "matchUpdateTypes": [
        "patch"
      ],
      "automerge": true
    },
    {
      "groupName": "zio-test",
      "matchPackageNames": [
        "/dev.zio:zio-test/"
      ]
    }
  ]
}
